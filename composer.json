{
  "name": "silverstripe-terraformers/gridfield-rich-filter-header",
  "description": "Rich filter header component from GridField",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe", "gridfield", "filter", "header"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "SilverStripe Ltd"
    },
    {
      "name": "Silverstripe Terraformers",
      "email": "terraformers@silverstripe.com"
    }
  ],
  "support": {
    "issues": "https://github.com/silverstripe-terraformers/gridfield-rich-filter-header/issues"
  },
  "require": {
    "php": "^8.1",
    "silverstripe/framework": "^5"
  },
  "require-dev": {
    "silverstripe/recipe-testing": "^3"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.x-dev",
      "dev-2": "2.x-dev"
    }
  },
  "scripts": {
    "lint": "phpcs src/ tests/php/",
    "lint-clean": "phpcbf src/ tests/php/"
  },
  "autoload": {
    "psr-4": {
      "Terraformers\\RichFilterHeader\\": "src/",
      "Terraformers\\RichFilterHeader\\Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/recipe-plugin": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
